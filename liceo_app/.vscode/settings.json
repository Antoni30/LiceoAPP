{
  "dart.flutterSdkPath": null,
  "dart.enableSdkFormatter": true,
  "dart.lineLength": 100,
  "editor.rulers": [100],
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "dart.debugSdkLibraries": false,
